{
    "name": "selfc-ext",
    "version": "1.0",
    "description": "SelfCAC Chrome Ext",
    "manifest_version": 2,
    "background": {
        "scripts": ["bundle.js"],
        "persistent": true
      },
    "permissions": [
        "<all_urls>",
        "webRequest",
        "tabs",
        "webRequestBlocking"
    ],
    "browser_action": {
      "default_title": "Check status",
      "default_popup": "popup.html",
      "default_icon": {
        "16":  "images/16.png",
        "32":  "images/32.png",
        "48":  "images/48.png",
        "128": "images/128.png"
      }
    }
  }